@model IEnumerable<GigimuDTO.PasienDTO>

@{
    ViewData["Title"] = "RiwayatKonsultasi";
}

<div class="row">
    <div class="col-lg-12">
        <!-- Basic Card Example -->
        <div class="card shadow mb-4">
            <div class="card-header py-3">
                <h6 class="m-0 font-weight-bold text-primary">Pasien List Page</h6>
            </div>
            <div class="card-body">
                <table class="table table-striped">
                    <tr>
                        <th>PasienID</th>
                        <th>Pasien Name</th>
                    </tr>
                    @foreach (var pasien in Model)
                    {
                        <tr>
                            <td>@pasien.PasienID</td>
                            <td>@pasien.Nama</td>
                        </tr>
                    }
                </table>

            </div>
        </div>

    </div>

</div>